money = float(input("введите сумму вклада: "))                    # вводим сумму вклада
per_cent = {'ТКБ': 5.6, 'СКБ': 5.9, 'ВТБ': 4.28, 'СБЕР': 4.0}     # задаем значения депозитных ставок для каждого банка
pr = list(per_cent.values())                                      # получаем список значений депозитных вкладов
p = map(float,pr)                                                 # преобразование каждого элемента списка в число с плавающей точкой
deposit = [money*0.01*i for i in p]                               # рассчитываем сумму прибыли по вкладу для каждого банка
deposit = list(map(round,deposit))                                # получаем список округленных значений прибыли
bank = list(per_cent.keys())                                      # преобразовываем в список ключи словаря
itog = dict(zip(bank,deposit))                                    # создаем новый словарь, где ключи по-прежнему банки, значения рассчетные цифры прибыли
print("Накопленная сумма, за год:")
for key,value in itog.items():                                    # распечатываем словарь по соответствию ключ-значение
    print(key,"-",value)


print("Mаксимальная сумма, которую вы можете заработать -",max(deposit))
